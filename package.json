    "cron": "2.3.1",
    "deepmerge-ts": "5.1.0",
    "eslint": "9.23.0",
    "express": "4.18.2",
    "grammy": "1.17.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "lowdb": "6.0.1",
    "prettier": "3.5.3",
    "puppeteer": "20.2.1",
    "ts-dedent": "2.2.0",
